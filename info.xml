<?xml version='1.0' encoding="ISO-8859-1"?>
<jtlshop3plugin>
    <Name>JTL MailChimp</Name>
    <Description>MailChimp Integration für den JTL-Shop 4</Description>
    <Author>Nico Schmitz</Author>
    <URL>http://www.jtl-software.de</URL>
    <XMLVersion>100</XMLVersion>
    <ShopVersion>400</ShopVersion>
    <PluginID>jtl_mailchimp</PluginID>
    <Install>
        <Version nr="100">
            <CreateDate>2013-08-11</CreateDate>
            <SQL>mailchimp.sql</SQL>
        </Version>
        <Version nr="101">
            <CreateDate>2015-08-02</CreateDate>
        </Version>
        <Version nr="102">
            <CreateDate>2016-08-15</CreateDate>
        </Version>
        <Hooks>
            <Hook id="38">removeSubscriber.php</Hook>
            <Hook id="39">addSubscriber.php</Hook>
            <Hook id="132">webhook.php</Hook>
        </Hooks>
        <Adminmenu>
            <Customlink sort="1">
                <Name>Abonnenten</Name>
                <Filename>abonnenten.php</Filename>
            </Customlink>
            <Customlink sort="2">
                <Name>Import</Name>
                <Filename>import.php</Filename>
            </Customlink>
            <Customlink sort="3">
                <Name>Einstellungen</Name>
                <Filename>settings.php</Filename>
            </Customlink>
        </Adminmenu>
    </Install>
</jtlshop3plugin>